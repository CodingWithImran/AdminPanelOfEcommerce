<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminPanelEcommerce"
        tools:targetApi="31">
        <activity
            android:name=".Activity.CartOrderActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DirectOrderActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.LuckDrawActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AddNewProductActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.NewProductActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AddAllProductActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AllProductActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AddCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AllCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>